      function Reajuste() {
        var nome1 = String(prompt("Digite o nome do funcionário: "));
        var telefone1 = parseFloat(prompt("Digite o telefone do funcionário (somente números): "));
        var salario1 = parseFloat(prompt("Digite o salário do funcionário: "));
        var porcentagem = parseFloat(prompt("Digite a porcentagem de reajuste salarial do funcionário: "));
        
        var funcionario = {
        nome: nome1 , telefone: telefone1 , 
        salario: salario1
      }
        console.log('my object:',funcionario)
        console.log(ReajusteS(salario1,porcentagem))
      }  
      
       var ReajusteS = (a,b) => a/b + a;
      
